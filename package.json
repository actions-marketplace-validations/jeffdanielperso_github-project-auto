{
  "name": "github-project-auto",
  "version": "1.0.0",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 0",
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "package": "ncc build --source-map --license licenses.txt",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test",
    "tag-latest": "git push origin :refs/tags/latest && git tag -fa latest -m \"Github Project Auto - Latest\" && git push origin master --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeffdanielperso/github-project-auto.git"
  },
  "keywords": [],
  "author": "Jeff Daniel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jeffdanielperso/github-project-auto/issues"
  },
  "homepage": "https://github.com/jeffdanielperso/github-project-auto#readme",
  "dependencies": {
    "@actions/core": "^1.3.0",
    "@actions/github": "^5.0.0",
    "@octokit/rest": "^18.5.3",
    "@octokit/types": "^6.16.0",
    "@types/lodash": "^4.14.170",
    "@typescript-eslint/parser": "^4.24.0",
    "@vercel/ncc": "^0.28.5",
    "eslint": "^7.27.0",
    "eslint-plugin-github": "^4.1.3",
    "eslint-plugin-prettier": "^3.4.0",
    "lodash": "^4.17.21",
    "prettier": "^2.3.0",
    "typescript": "^4.2.4"
  }
}
